{
  "name": "gulp-html2pug",
  "version": "4.0.0",
  "description": "Gulp plugin to convert html files to pug",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --cache .",
    "test": "mocha",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/gulp-html2pug.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "pug",
    "html",
    "templates"
  ],
  "author": "Julian Grinblat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-html2pug/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-html2pug#readme",
  "dependencies": {
    "html2pug": "^4.0.0",
    "plugin-error": "^1.0.1",
    "promise-toolbox": "^0.20.0",
    "through2": "^4.0.2",
    "vinyl-contents-tostring": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/plugin-proposal-partial-application": "^7.16.0",
    "@babel/plugin-proposal-throw-expressions": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.3",
    "rollup": "^2.59.0",
    "sinon": "^12.0.0",
    "sinon-chai": "^3.7.0",
    "vinyl": "^2.2.1"
  }
}
