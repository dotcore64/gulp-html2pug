    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "mocha": "^11.7.4",
    "p-event": "^7.0.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.1",
    "vinyl": "^3.0.1"
