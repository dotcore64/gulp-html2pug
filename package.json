{
  "name": "gulp-html2pug",
  "version": "1.1.0",
  "description": "Gulp plugin to convert html files to pug",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint 'src/*.js' 'test/*.js'",
    "pretest": "npm run lint",
    "test": "mocha",
    "build": "babel src -d dist",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrin4869/gulp-html2pug.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "pug",
    "html",
    "templates"
  ],
  "author": "Julian Grinblat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perrin4869/gulp-html2pug/issues"
  },
  "homepage": "https://github.com/perrin4869/gulp-html2pug#readme",
  "dependencies": {
    "html2pug": "^2.0.1",
    "plugin-error": "^1.0.1",
    "through2": "^2.0.3",
    "vinyl-contents-tostring": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "event-stream": "^3.3.4",
    "mocha": "^5.2.0",
    "nyc": "^11.2.1",
    "vinyl": "^1.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
