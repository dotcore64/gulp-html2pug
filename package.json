{
  "name": "gulp-html2pug",
  "version": "0.0.2",
  "description": "Gulp plugin to convert html files to pug",
  "main": "dist/index.js",
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "cover": "istanbul cover --report text --report html node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrin4869/gulp-html2pug.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "pug",
    "html",
    "templates"
  ],
  "author": "Julian Grinblat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perrin4869/gulp-html2pug/issues"
  },
  "homepage": "https://github.com/perrin4869/gulp-html2pug#readme",
  "dependencies": {
    "gulp-util": "^3.0.7",
    "html2pug": "0.0.1",
    "through2": "^2.0.1",
    "vinyl-contents-tostring": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.2",
    "vinyl": "^2.0.2"
  }
}
